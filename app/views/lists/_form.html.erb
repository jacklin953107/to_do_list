<% if @list.errors.present? %>
    <ul>
      <% @list.errors.each do |error, message|%>
          <li><%= "#{error.capitalize} #{message}"%></li>
      <% end %>
    </ul>
<% end %>
<div class="form container">
<%= form_for @list do |f| %>
  <div class="form-group">
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :due, "DueDay" %>
    <%= f.text_field :due %>
  </div>
  <div class="form-group">
    <%= f.label :note, "Note" %>
    <%= f.text_field :note %>
  </div>
    <%= f.submit %>
<% end %>
</div>